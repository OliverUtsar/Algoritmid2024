def binaar_otsing(massiiv, sihtväärtus):
    vasak, parem = 0, len(massiiv) - 1
    while vasak <= parem:
        keskmine = (vasak + parem) // 2
        if massiiv[keskmine] == sihtväärtus:
            return keskmine
        # Sihtväärtus leitud
        elif massiiv[keskmine] < sihtväärtus:
            vasak = keskmine + 1
        else:
            parem = keskmine - 1
    return -1
    # SIhtväärtus ei ole leitud
